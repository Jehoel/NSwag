{% if parameter.IsDateArray -%}
urlBuilder_.Replace("{{ "{" }}{{ parameter.Name }}}", Uri.EscapeDataString(String.Join(",", Enumerable.Select({{ parameter.VariableName }}, s_ => s_.ToString("{{ ParameterDateTimeFormat }}", CultureInfo.InvariantCulture)))));
{% elseif parameter.IsDate -%}
urlBuilder_.Replace("{{ "{" }}{{ parameter.Name }}}", Uri.EscapeDataString({{ parameter.VariableName }}.ToString("{{ ParameterDateTimeFormat }}", CultureInfo.InvariantCulture)));
{% elseif parameter.IsArray -%}
urlBuilder_.Replace("{{ "{" }}{{ parameter.Name }}}", Uri.EscapeDataString(String.Join(",", Enumerable.Select({{ parameter.VariableName }}, s_ => ConvertToString(s_, CultureInfo.InvariantCulture)))));
{% else -%}
urlBuilder_.Replace("{{ "{" }}{{ parameter.Name }}}", Uri.EscapeDataString(ConvertToString({{ parameter.VariableName }}, CultureInfo.InvariantCulture)));
{% endif -%}